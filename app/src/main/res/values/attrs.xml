<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EngineSelectItem">
        <attr name="egsTitle" format="string"/>
        <attr name="egsIcon" format="reference"/>
        <attr name="egsSelect" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="AccessSelectItem">
        <attr name="asiIcon" format="reference"/>
        <attr name="asiTitle" format="string"/>
        <attr name="asiSelect" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="BrowserMenuItem">
        <attr name="bmiIcon" format="reference"/>
        <attr name="bmiTitle" format="string"/>
    </declare-styleable>
    
    <declare-styleable name="TagTabItem">
        <attr name="ttiTitle" format="string"/>
        <attr name="ttiCount" format="integer"/>
        <attr name="ttiSelect" format="boolean"/>
    </declare-styleable>
